{
  "name": "nest-blogmatica",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:dev": "yarn dev",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js -f src/database/cli/config.ts ",
    "db:migration:generate": "yarn db migration:generate",
    "db:migration:create": "yarn db migration:create",
    "db:migration:revert": "yarn db migration:revert",
    "db:migrate": "yarn db migration:run",
    "postinstall": "yarn build"
  },
  "dependencies": {
    "@aws-crypto/client-node": "^1.0.5",
    "@nestjs/common": "^7.0.0",
    "@nestjs/config": "^0.4.2",
    "@nestjs/core": "^7.0.0",
    "@nestjs/graphql": "^7.3.4",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.1",
    "@nestjs/typeorm": "^7.0.0",
    "apollo-server-express": "^2.14.2",
    "bcrypt": "^4.0.1",
    "cookie-parser": "^1.4.5",
    "graphql": "^15.0.0",
    "graphql-tools": "^5.0.0",
    "js-base64": "^2.5.2",
    "lodash": "^4.17.19",
    "luxon": "^1.23.0",
    "md5": "^2.2.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.3",
    "plaid": "^5.1.0",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.3",
    "@types/jest": "25.1.4",
    "@types/js-base64": "^2.3.1",
    "@types/lodash": "^4.14.150",
    "@types/luxon": "^1.22.0",
    "@types/md5": "^2.2.0",
    "@types/node": "^13.9.1",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-local": "^1.0.33",
    "@types/source-map-support": "^0.5.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^25.5.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.9.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4",
    "yarn": "^1.22.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
